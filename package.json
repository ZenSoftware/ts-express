{
  "name": "ts-express",
  "version": "1.0.0",
  "author": "",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/main.ts",
    "start:watch": "tsc-watch --onSuccess \"node --inspect=5858 dist/main.js\"",
    "build": "webpack",
    "docker:build": "npm run build && docker build -f ./Dockerfile -t ts-express .",
    "docker:run": "docker run -p 3001:3000 -d ts-express"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.4",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tsc-watch": "^3.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
